<%= content_for :head, auto_discovery_link_tag(:atom, space_tiddler_revisions_path(@space, @tiddler, format: :atom), title: "Federacy: All Revisions for \"#{@tiddler.title}\" in #{@space}") %>
<% content_for :title do %>
  All Revisions - <%= @tiddler.title %> - <%= @space %> - Federacy
<% end %>

<main>
  <h1>All Revisions for <%= @tiddler.title %></h1>

  <ul class="revisions">
    <% @revisions.each do |revision| %>
      <li class="revision">
        <%= link_to html_path(:space_tiddler_revision_path, revision.tiddler.space, revision.tiddler, revision) do %>
          <%= revision.title %><% end %>
        [<em><%= revision.created_at.strftime "%H:%M %d %B %Y" %></em>]
      </li>
    <% end %>
  </ul>
</main>


